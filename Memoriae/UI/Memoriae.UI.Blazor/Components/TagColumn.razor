<div style="text-align: right; margin-right: 10px;">
    @foreach (var tag in Tags)
    {
        <div><a id="tag" href="" style=" text-align: right;">@($"{tag.Name}")</a></div>
    }
</div>

@code{
    [Parameter]
    public IEnumerable<Tag> Tags { get; set; }
}